
/* input */
input.text { border:1px solid #cfcfcf;  background-color:#ffffff; }
textarea.comm { border:1px solid #cbcaca; font-size:12px; height:58px; color:#979797; width:99%; scrollbar-face-color:#FFFFFF;
scrollbar-highlight-color: #FFFFFF; scrollbar-3dlight-color: #FFFFFF; scrollbar-shadow-color:#FFFFFF; scrollbar-darkshadow-color: #FFFFFF; scrollbar-track-color: #FFFFFF; scrollbar-arrow-color: #FFFFFF} 
textarea.for_txt { border:0; width:100%; color:#979797;}
input.search { border:1px solid #DDD;}
select.sel1 { border:1px solid #CCC;}
select.slt1 {height:25px; line-height:25px;}


/* a */
a:hover.lnk { text-decoration:underline; color:#C90;}
a.link_fff {font-size:12px; color:#a6a6a6; text-decoration:none;}


.hide { visibility:hidden; overflow:hidden; position:absolute; text-indent:-3000em; width:0; height:0; font-size:0; line-height:0; }
.show {visibility:show;}
.b { font-weight:bold;}
.off {display: none;}
.on {display: block;}
.clear { clear:both}
.cleft {clear:left;;}
.cright {clear:right;}
.fleft { float:left;}
.fright { float:right;}
.textcen { text-align:center;}
.textleft { text-align:left;}
.textright { text-align:right;}
.backoff {background:none;}
.bold {font-weight:bold;}
.vmid {vertical-align:middle;}
.wdhfull {width:100%;}

/* border */
.bd_non {border:none;}
.bd_tp {border-top:1px solid #CCC;}
.bd_btm {border-bottom:1px solid #CCC;}
.bd_dot_btm {border-bottom:1px dotted #CCC;}
.bd_dot_tp {border-top:1px dotted #CCC;}

/* font_color */
.font_ylw { color:#e9ae20;}
.font_bgrn { color:#0e6a59}
.font_blk {color:#000}
.font_grn {color:#2aa00f}
.font_red {color:#ff0000}
.font_red2 {color:#b10000;}
.font_blue {color:#0099cc}
.font_beg {color:#efeed9}


/* rollover button */
.rbtn {}
.rbtn a img { display:inline;}
.rbtn a img.over { display:none;}
.rbtn a:hover { position:relative;}
.rbtn a:hover img { display:none;}
.rbtn a:hover img.over { display:inline;}


/* 라운딩 박스 */
.roundbox {
	border:1px solid #cccccc;
	-webkit-border-radius: 4px;
	   -moz-border-radius: 4px;
	        border-radius: 4px;
	-webkit-box-shadow:1px 1px 1px rgba(255, 255, 255, 0.9) inset;
       -moz-box-shadow:1px 1px 1px rgba(255, 255, 255, 0.9) inset;
            box-shadow:1px 1px 1px rgba(255, 255, 255, 0.9) inset;
	behavior: url(../common/js/PIE.htc);
}


